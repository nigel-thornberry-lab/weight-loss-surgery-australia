---
import BaseLayout from '../../layouts/BaseLayout.astro';
import { getCollection } from 'astro:content';

const surgeons = await getCollection('surgeons');
const frankstonSurgeons = surgeons.filter((s: any) => 
	s.data.location?.includes('Frankston') || 
	s.data.location?.includes('Dandenong') ||
	s.data.suburbs?.includes('Frankston') ||
	s.data.suburbs?.includes('Mornington Peninsula')
);

const title = "Weight Loss Surgery Frankston VIC | Expert Bariatric Surgeons";
const description = "Find weight loss surgery in Frankston with comprehensive local support. Expert surgeons at Peninsula Private Hospital, local dietitians, and detailed Medicare information for Frankston and Mornington Peninsula residents.";
