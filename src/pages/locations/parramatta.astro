const surgeons = await getCollection('surgeons');
const parramattaSurgeons = surgeons.filter((s: any) => 
	s.data.location?.includes('Parramatta') || 
	s.data.location?.includes('Westmead') ||
	s.data.suburbs?.includes('Parramatta')
