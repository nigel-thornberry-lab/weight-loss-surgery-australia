const surgeons = await getCollection('surgeons');
const manlySurgeons = surgeons.filter((s: any) => 
	s.data.suburbs?.includes('Manly') || 
	s.data.location?.includes('Northern Beaches') ||
	s.data.suburbs?.includes('Frenchs Forest')
);

const title = "Weight Loss Surgery Manly NSW | Northern Beaches Bariatric Surgeons";
