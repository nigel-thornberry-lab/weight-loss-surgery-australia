---
import BaseLayout from '../../layouts/BaseLayout.astro';
import { getCollection } from 'astro:content';

const surgeons = await getCollection('surgeons');
const blacktownSurgeons = surgeons.filter((s: any) =>
	s.data.location?.includes('Blacktown') ||
	s.data.suburbs?.includes('Blacktown')
);

const title = "Weight Loss Surgery Blacktown NSW | Expert Bariatric Surgeons";
const description = "Find weight loss surgery in Blacktown with comprehensive local support. Expert surgeons, local dietitians, and detailed Medicare information for Blacktown residents.";
---

<BaseLayout title={title} description={description}>
	<!-- Breadcrumbs -->
	<div class="bg-gray-50 border-b border-gray-200">
		<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-4">
			<nav class="flex text-sm" aria-label="Breadcrumb">
				<a href="/" class="text-blue-600 hover:text-blue-800">Home</a>
				<span class="mx-2 text-gray-400">/</span>
				<a href="/locations/sydney" class="text-blue-600 hover:text-blue-800">Sydney</a>
				<span class="mx-2 text-gray-400">/</span>
				<span class="text-gray-600">Blacktown</span>
			</nav>
		</div>
	</div>

	<!-- Hero Section -->
	<section class="bg-gradient-to-br from-blue-600 to-blue-700 text-white py-16">
		<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
			<div class="max-w-4xl">
				<h1 class="text-4xl md:text-5xl font-bold mb-6">
					Weight Loss Surgery in Blacktown
				</h1>
				<p class="text-xl text-blue-100 mb-8 leading-relaxed">
					Comprehensive bariatric surgery services for Blacktown residents. Expert surgeons, convenient local hospitals, and complete support services.
				</p>
				<div class="flex flex-col sm:flex-row gap-4">
					<a href="/sydney/consultation" class="bg-white text-blue-600 px-8 py-4 rounded-lg hover:bg-blue-50 transition text-lg font-semibold text-center">
						Book Free Consultation
					</a>
					<a href="/sydney/cost" class="bg-blue-800 text-white px-8 py-4 rounded-lg hover:bg-blue-900 transition text-lg font-semibold text-center border-2 border-white/20">
						View Cost Breakdown
					</a>
				</div>
			</div>
		</div>
	</section>

	<!-- Key Benefits -->
	<section class="py-16 bg-white">
		<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
			<h2 class="text-3xl font-bold text-gray-900 mb-12 text-center">Why Choose Blacktown for Bariatric Surgery</h2>

			<div class="grid md:grid-cols-3 gap-8">
				<div class="bg-blue-50 rounded-xl p-6 border border-blue-100">
					<div class="w-12 h-12 bg-blue-600 rounded-lg flex items-center justify-center mb-4">
						<svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4" />
						</svg>
					</div>
					<h3 class="text-xl font-bold text-gray-900 mb-3">Major Transport Hub</h3>
					<p class="text-gray-700 mb-3">Blacktown is Sydney's largest transport interchange outside the CBD - easy access to hospitals and follow-up appointments.</p>
					<p class="text-sm text-blue-600 font-medium">→ Excellent train, bus, and road connections</p>
				</div>

				<div class="bg-green-50 rounded-xl p-6 border border-green-100">
					<div class="w-12 h-12 bg-green-600 rounded-lg flex items-center justify-center mb-4">
						<svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z" />
						</svg>
					</div>
					<h3 class="text-xl font-bold text-gray-900 mb-3">Multicultural Care</h3>
					<p class="text-gray-700 mb-3">Blacktown's diverse community benefits from culturally-sensitive healthcare with multilingual support services.</p>
					<p class="text-sm text-green-600 font-medium">→ Support in multiple languages</p>
				</div>

				<div class="bg-purple-50 rounded-xl p-6 border border-purple-100">
					<div class="w-12 h-12 bg-purple-600 rounded-lg flex items-center justify-center mb-4">
						<svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z" />
						</svg>
					</div>
					<h3 class="text-xl font-bold text-gray-900 mb-3">Premium Hospitals Nearby</h3>
					<p class="text-gray-700 mb-3">Access to Westmead Private Hospital (10 min) and Blacktown Hospital - both with experienced bariatric teams.</p>
					<p class="text-sm text-purple-600 font-medium">→ Quality care close to home</p>
				</div>
			</div>
		</div>
	</section>

	<!-- Hospital & Surgery Location Details -->
	<section class="py-16 bg-gray-50">
		<div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
			<h2 class="text-3xl font-bold text-gray-900 mb-4 text-center">Your Surgery Location</h2>
			<p class="text-center text-gray-600 mb-12 max-w-3xl mx-auto">Blacktown residents have easy access to premium private hospitals in Western Sydney for bariatric surgery.</p>
			
			<div class="grid lg:grid-cols-2 gap-8">
				<!-- Hospital Information -->
				<div class="bg-white rounded-xl shadow-md p-8">
					<div class="flex items-start mb-6">
						<div class="w-12 h-12 bg-blue-100 rounded-lg flex items-center justify-center mr-4 flex-shrink-0">
							<svg class="w-6 h-6 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
								<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4" />
							</svg>
						</div>
						<div>
							<a href="https://www.google.com/maps/search/?api=1&query=Westmead+Private+Hospital" target="_blank" rel="noopener noreferrer">
