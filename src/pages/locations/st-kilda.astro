---
import BaseLayout from '../../layouts/BaseLayout.astro';
import { getCollection } from 'astro:content';

const surgeons = await getCollection('surgeons');
const stKildaSurgeons = surgeons.filter((s: any) =>
	s.data.location?.includes('St Kilda') ||
	s.data.suburbs?.includes('St Kilda') ||
	s.data.hospitals?.includes('Cabrini St Kilda')
);

const title = "Weight Loss Surgery St Kilda VIC | Expert Bariatric Surgeons";
const description = "Find weight loss surgery in St Kilda with comprehensive local support. Expert surgeons at Cabrini St Kilda, and complete support for St Kilda residents.";
const canonicalUrl = "https://weight-loss-surgery-australia.com.au/locations/st-kilda";
