---
import BaseLayout from '../../layouts/BaseLayout.astro';
import { getCollection } from 'astro:content';

const surgeons = await getCollection('surgeons');
const dandenongSurgeons = surgeons.filter((s: any) => 
	s.data.location?.includes('Dandenong') || 
	s.data.location?.includes('Berwick') ||
	s.data.suburbs?.includes('Dandenong') ||
	s.data.suburbs?.includes('Southeast Melbourne')
);

const title = "Weight Loss Surgery Dandenong VIC | Expert Bariatric Surgeons";
const description = "Find weight loss surgery in Dandenong with comprehensive local support. Expert surgeons, modern hospital facilities, and detailed Medicare information for Dandenong and Southeast Melbourne residents.";
