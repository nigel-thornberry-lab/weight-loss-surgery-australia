					"@type": "MedicalWebPage",
					"@id": "https://weight-loss-surgery-australia-a0qxp09x9.vercel.app/procedures/gastric-band#webpage",
					"url": "https://weight-loss-surgery-australia-a0qxp09x9.vercel.app/procedures/gastric-band",
					"name": "Gastric Band Surgery Australia - Complete Guide",
					"description": "Comprehensive guide to gastric band surgery in Australia including costs, recovery, eligibility and finding qualified surgeons.",
					"inLanguage": "en-AU",
					"datePublished": "2025-01-03",
					"dateModified": "2025-01-03",
